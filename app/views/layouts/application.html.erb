<html xmlns="http://www.w3.org/1999/xhtml">
  <head>
    <meta http-equiv="content-type" content="text/html; charset=utf-8" />
    <title>Chapter Admin</title>
    <link rel="shortcut icon" href="../images/favicon.ico" type="image/x-icon" />
    <%= stylesheet_link_tag 'default' %>
    <%= javascript_include_tag :defaults %>
    <%= calendar_date_select_includes %>
    <%= csrf_meta_tag %>
  </head>
  <!-- start body -->
  <body>
    <!-- start header -->
    <!-- start logo -->
    <div id="logo"><%= image_tag("../images/evhog-logo.png ", :size => "90x90") %> </div>
    <!-- end logo -->
    <!-- start menu -->
    <div id="menu">
      <%= yield :menu %>
    </div>
    <!-- end menu -->
    <!-- end header -->

    <!-- start wrapper -->
    <div id="wrapper">

      <!-- start page -->
      <%= yield :main_page %>
      <!-- end page -->

    </div>
    <!-- end wrapper -->

    <!-- start footer -->
    <div id="footer">
	<%= yield :footer %>
    </div>
    <!-- end footer -->

  </body>
  <!-- end body -->

</html>
<!-- end html -->
